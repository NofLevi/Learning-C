#include <stdio.h>

typedef struct cars{
	char manufacture[15];
	int year;
	int engine;
	int price;
}cars;

void getinfo(struct cars car[])
{
	int i;
	for(i=0;i<5;i++)
	{
	puts("Enter 1.Manufacture 2.year 3.engine power 4.price:");
	scanf("%s",&car[i].manufacture);
	scanf("%d",&car[i].year);
	scanf("%d",&car[i].engine);
	scanf("%d",&car[i].price);
	}
}
float average(struct cars car[])
{
	int i;
	float average=0;
	for(i=0;i<5;i++)
	{
		average+=car[i].price;
	}
	average = average / i;
	return average;
	
}

int main()
{
	float averageprice;
	cars car[5];
	getinfo(car);
	averageprice = average(car);
	printf("the average price is: %f\n",averageprice);
}

