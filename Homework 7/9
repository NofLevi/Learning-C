#include <stdio.h>
void main()
{
    int a,times,temp,sum;
    times = 0;
    sum = 0;
    printf("Enter 1 Positive Integer");
    scanf("%d",&a);
    while (times<10)
    {
        temp = a % 10;
        sum = sum + temp;
        a = a / 10;
        times = times + 1;
    }
    printf("The sum of this number is:%d",sum);
    
}
