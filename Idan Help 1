    printf("Enter Numbers 2:  ") ;
    scanf("%d",&b);
   
    printf("%d\n",a+b);
    if(a+b==15)
    {
    printf("hello");
    }
    else if (a+b==10)
 
    {
        printf("noob");
    }
    else
    {
        printf("salam alekum");
    }
