#include <stdio.h>
char line()
{
	printf("--------------------------------------------------\n");
}

int get_num()//check function
{
	int a;
	line();
	printf("Enter a integer between 1 to 9: ");
	scanf("%c",&a);
	while (a<48 || a>57)
	{
	printf("Enter a integer between 1 to 9: ");
	scanf("%c",&a);
	}
	return a;
}
int main()
{
	int a;	
	a = get_num();
	printf("%d\n",a-48);
	line();
}
