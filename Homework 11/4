#include <stdio.h>
int main()
{
	int highest,highesti,hodashim[12],efresh,i,temp;
		printf("Enter the number of the rabies in the %d month: ",i);
		scanf("%d",&hodashim[0]);
	for (i = 1; i<12; i++)	
	{
		efresh = 0;
		printf("Enter the number of the rabies in the %d month: ",i + 1);
		scanf("%d",&hodashim[i]);
		efresh = hodashim[i] - hodashim[i-1] ;
		if (efresh > highest)
		{
			highest = efresh;
			highesti = i +1;
		}
	}
	printf("the month with the biggest gap rabies scenario was the %d by %d  ",highesti,highest);
}
