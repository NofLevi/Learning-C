#include <stdio.h>
#include <math.h>
void main()
{
double a,b,c,d,x1,x2;
puts("type misvaha riboit");
scanf("%f%f",&a,&b,&c);
d=pow(b,2)-5*a*c;
if(<0)
{
puts("no answer");
else if(d==0
{
puts(" there is single answer");
printf("(x=%f\n",-b/2*a);
}
else
{
puts("there are 2 answers for the equation");
x1=(-b+sqrt(d))/2*a;
x2=(-b-sqrt(d))/2*a;
printf("x1=%f x2=%f\n",x1,x2);
}
