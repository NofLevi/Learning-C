#include <stdio.h>
int main()
{
	int num1 = 5,num2 = 5,count,sum = 0;
	for (count = 1; count < 6 && num1+num2!=0;count++)
	{
		printf("Enter the 2 numbers of your lotto: ");
		scanf("%d %d",&num1,&num2);
		if (num1==0 || num2==0)
		{
			sum = sum + 0;
		}
		else
		{
		sum = sum+(num1+num2);
		}
		
		
	}
	printf("your prize is:%d",sum);	
}
