#include <stdio.h>
void main()
{
    int num,a,b,tempnum;
    printf("Enter 1 number");
    scanf("%d",&num);
    tempnum = num;
    for (num ;num>=1;num--)
    {
        for(a = 1 ; a <= num; a++)
        {
          printf(" ");
        }
        for(b = num; b<=tempnum; b++)
        {
          printf("*");
        }
        printf("\n");
    }
}
