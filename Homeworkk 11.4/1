#include <stdio.h>
void main()
{
	int highest = 0,hodashim[12],mikreikalevet,i;
	for (i = 0; i<12; i++)	
	{
		printf("Enter the number of the rabies in the %d month: ",i + 1);
		scanf("%d",&hodashim[i]);
		if (hodashim[i]>highest)
		{
			highest = i;
		}
	}
	printf("the month with the highest rabies scenario was the %d ",highest);
}
