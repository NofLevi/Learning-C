#include <stdio.h>
void main()
{
    int a,sum,temp;
    temp = 10;
    sum= 0;
    while (temp>0)
    {
        printf("Enter a number: ");
        scanf("%d",&a);
        if (a>0)
        {
            sum = sum + a;
            temp = temp - 1;
            
        }
        else 
        {
            temp = temp -1;
        }
    }
    printf("the sum of the positive numbers is:%d",sum);
}
