#include <stdio.h>
void main()
{
    int a,temp,sum;
    temp = 9 ;
    sum = 0;
    printf("Enter a number");
    scanf("%d",&a);
    while (temp>0)
    {
        if (a>0)
        {
        sum = sum + a;
        temp = temp - 1;
        printf("Enter a number");
        scanf("%d",&a);
        }
        else if (a<0)
        {
            a = 0;
        temp = temp -1;
        printf("Enter a number");
        scanf("%d",&a);
        }
    }
    printf("The sum is:%d",sum);
}
