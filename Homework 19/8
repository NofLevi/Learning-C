#include <stdio.h>
int main()
{
	int metrics[4][4]={{1,0,-8,2},{8,0,1,0},{4,4,4,4},{3,10,5,1}};
	int i,j,sum=0,temp=0,flag = 0,bugfix;
	for (i=0;i<4;i++)
	{
		temp = 0;
		for(j=0;j<4;j++)//problem when minus number
		{
			if(metrics[i][j]>=0)
			{
				temp+=metrics[i][j];
			}
			else
			{
				bugfix = metrics[i][j]*-1;
				temp-=bugfix;
			}
		}
		if (temp>sum)
		{
			sum=temp;
		}
		else if(sum>=temp)
		{
			flag+=1;
		}
	}
	printf("%d",flag);
}
