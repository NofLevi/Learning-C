#include <stdio.h>
int main()
{
	int metrics[4][4]={{1,0,-8,2},{8,0,1,0},{4,4,4,4},{3,10,5,1}};
	int i,j,sum=0,temp,flag = 0;
	for (i=0;i<4;i++)
	{
		temp = 0;
		for(j=0;j<4;j++)
		{
			temp+=metrics[i][j];
		}
		if (temp>sum)
		{
			sum=temp;
		}
		else if(sum>=temp)
		{
			flag+=1;
		}
	}
	if(flag=0)
	{
	printf("the metrics is growthing");
	}
	else if(flag>0)
	{
	printf("the metrics isnt growthing kind of metrics");
	}
}
