#include <stdio.h>
#define N 3
int check(int array[N][N])
{
	int sum=0,i,j,temp=0,flag=0;
	for(i=0;i<N;i++)//finding the sum+alahson smol le yamin [work]
	{
		sum+=array[i][i];
	}
	for(i=0;i<N;i++) // checking the lines [work]
	{
		temp = 0;
		for(j=0;j<N;j++)
		{
			temp+=array[i][j];
		}
		if(temp!=sum)
		flag+=1;
	}
	for(j=0;j<N;j++) // checking the aamodot [work]
	{
		temp = 0;
		for(i=0;i<N;i++)
		{
			temp+=array[i][j];
		}
		if(temp!=sum)
		flag+=1;
	}
	temp=0;
	for(i=0;i<N;i++)
	{
		temp+=array[i][3-i];
	}
	if(temp!=sum)
	flag+=1;
	return flag;
}
int main()
{
	int array[N][N],i,j,value;
	printf("enter a metrics: ");
	for (i=0;i<N;i++)
	{
		for (j=0;j<N;j++)
		{
			scanf("%d",&array[i][j]);
		}
	}
	value = check(array);
	printf("%d",value);
}
