#include <stdio.h>
#define N 3
int check(int array[N][N])
{
	int temp,sum,value,flag=0,i,j;
	for(i=0;i<N;i++)//bodek shorot [work][need to be 1]
	{
		temp = 0;
		for(j=0;j<N;j++)
		{
			temp+=array[i][j];
		}
		if(temp!=sum)
		{
			sum = temp;
			flag +=1;
		}
	}
	for (j=0;j<N;j++)//bodek amodot[work]
	{
		temp = 0;
		for(i=0;i<N;i++)
		{
			temp+=array[i][j];
		}
	}
	if(temp!=sum)
	{
		flag+=1;
	}
	temp=0;
	for(i=0;i<N;i++)//bodek alahson smol le yamin
	{
		temp+=array[i][i];
	}
	if(temp!=sum)
	{
		flag+=1;
	}
	temp=0;
	for(i=3;i>0;i--)
	{
		temp+=array[i][i];
	}
	if(temp!=sum)
	{
		flag+=1;
	}
	return value;
}
int main()
{
	int array[N][N],i,j,value;
	printf("enter a metrics: ");
	for (i=0;i<N;i++)
	{
		for (j=0;j<N;j++)
		{
			scanf("%d",&array[i][j]);
		}
	}
	value = check(array);
	printf("%d",value);
}
