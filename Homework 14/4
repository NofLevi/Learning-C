#include <stdio.h>
#include <string.h>
int main()
{
    char str[20],tempchar;
    int i,x,temp;
    LOOP:
    printf("Enter a number: ");
    gets(str);
    x = strlen(str);
    for (i=0;i<=x;i++)
    {    
        tempchar = str[i];
        temp = tempchar;
        printf("%d",temp);
        if ((temp>=48)&&(temp<=57))
        {
        str[i] = str[i] - 48;
        }
        else
        {
        printf("Error: dont put a letters or symbols in it only numbers\n");
        goto LOOP;
        }
    }
    for (i=0;i<x;i++)
    {
        printf("%d",str[i]);
    }
}
