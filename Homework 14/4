#include <stdio.h>
#include <string.h>
int main()
{
	char str[20];
	int i,x;
	LOOP:
	printf("Enter a number");
	gets(str);
	x = strlen(str);
	for (i=0;i<=x;i++)
	{	
		if (str[i]>=48 && str[i]<=57)
		{
		str[i] = str[i] - 48;
		}
		else
		{
			printf("Error: dont put a letters or symbols in it only numbers");
			goto LOOP;
		}
	}
	for ()
}
