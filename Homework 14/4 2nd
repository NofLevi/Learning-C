#include <stdio.h>
#include <string.h>
int main()
{
	char str[20];
	int i,array[20],x;
	LOOP:
	printf("Enter a number: ");
	gets(str);
	x= strlen(str);
	for (i=0;i<=x;i++)
	{
		if (str[i]>=48 && str[i]<=57)
		{
			array[i]= str[i];
			array[i] -= 48;
		}
		else 
		{
			printf("Error:i asked you to put a number i give you 1 more chance\n");
			goto LOOP;
		}
	}
	for (i=0;i<=x;i++)
	{
		printf("%d",array[i]);
	}
}
