#include <stdio.h>
void main()
{
	int a,b,c;
	printf("type 3 numbers: ");
	scanf("%d%d%d",&a,&b,&c);
	if ((a>b)&&(a>c))
	{
		printf("the first number is the biggest: %d",a);		
	}
	else if((b>a)&&(b>c))
	{
		printf("the biggest number is the second: %d",b);
	}
    else if ((c>a)&&(c>b))
    {
    	printf("the biggest number is the third: %d",c);
	}
}
